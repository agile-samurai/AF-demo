    new S3DataService("rdso-challenge2", "test", "us-east-1", "dossier-storage",
                client("AKIAWL3TNIWMJEZOGXSG", "VAJxNKY7+Rgd8pr9MmpoP43dXIB+ccbUHkWpUEl6", "us-east-1"), new ObjectMapper());

                public S3Client client(String accessKeyId, String secretAccessKey, String regionAsString) {
        return S3Client.builder()
                .region(Region.of(regionAsString))
                .credentialsProvider(StaticCredentialsProvider
                        .create(AwsBasicCredentials.create(accessKeyId,
                                secretAccessKey))).build();
    }
